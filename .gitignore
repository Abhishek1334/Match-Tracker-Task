# Root-level .gitignore for match-tracker
node_modules/

# Ignore node_modules in both frontend and backend
match-tracker-frontend/node_modules/
match-tracker-backend/node_modules/

# Ignore build/dist folders if any
match-tracker-frontend/dist/
match-tracker-backend/dist/
match-tracker-frontend/build/
match-tracker-backend/build/

# Ignore environment files
match-tracker-frontend/.env
match-tracker-backend/.env

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Ignore system files
.DS_Store
Thumbs.db

# Ignore IDE/editor configs
.vscode/
.idea/
*.swp

# Ignore coverage (if using Jest)
match-tracker-backend/coverage/

# Ignore test results or snapshots
**/__snapshots__/
**/*.test.js.snap

# Optional: lock files if using yarn/pnpm interchangeably
yarn.lock
pnpm-lock.yaml

# Optional: ignore output from Swagger docs (if generated)
match-tracker-backend/swagger-output.json
